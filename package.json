{
  "name": "binb",
  "version": "1.0.6",
  "description": "Competitive, multiplayer, realtime, guess the song game",
  "homepage": "https://binb.co",
  "bugs": "https://github.com/lpinca/binb/issues",
  "repository": "lpinca/binb",
