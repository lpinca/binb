    "express": "4.15.x",
    "express-session": "1.15.x",
    "faye-websocket": "0.11.x",
    "forwarded-for": "1.0.x",
    "jade": "1.11.x",
    "nodemailer": "4.0.x",
    "primus": "7.0.x",
    "primus-emitter": "3.1.x",
    "primus-rooms": "3.4.x",
    "redis": "2.7.x",
    "serve-favicon": "2.4.x",
